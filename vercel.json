{
  "version": 2,
  "builds": [
    {
      "src": "dist/polling-system/server/server.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/polling-system/server/server.mjs"
    }
  ],
  "functions": {
    "dist/polling-system/server/server.mjs": {
      "runtime": "nodejs18.x"
    }
  }
}
